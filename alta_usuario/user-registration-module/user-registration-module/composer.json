{
    "name": "user-registration-module",
    "description": "A user registration module using PHP, MySQL, Bootstrap 5, and AdminLTE.",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "nesbot/carbon": "^2.53",
        "guzzlehttp/guzzle": "^7.0",
        "twilio/sdk": "^6.0",
        "twbs/bootstrap": "^5.0",
        "almasaeed2010/adminlte": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}